<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="ProjectCommandSet_en-us">
    <CommandPrefix>Project</CommandPrefix> 
    <Example>Launch</Example>
    
 
     <Command Name="playVideo">
      <Example>play a video</Example>
      <ListenFor>play video {video}</ListenFor>
      <Feedback>Playing the video</Feedback>
      <Navigate/>
    </Command>
    
    <Command Name="playMusic">
      <Example>play some music</Example>
      <ListenFor>play music {music}</ListenFor>
      <Feedback>Playing music</Feedback>
      <Navigate/>
    </Command>

    <Command Name="pausePlayer">
      <Example>Pause player</Example>
      <ListenFor>pause [the] [video] [music]</ListenFor>
      <Feedback>Pausing</Feedback>
      <Navigate/>
    </Command>
    
    <Command Name="resumePlayer">
      <Example>Resume content</Example>
      <ListenFor>resume [the] [video] [music]</ListenFor>
      <Feedback>Resuming</Feedback>
      <Navigate/>
    </Command>
    
    <Command Name="fastForwardPlayer">
      <Example>Fast forward content</Example>
      <ListenFor>fast forward the [video] [music]</ListenFor>
      <Feedback>Fast forwarding</Feedback>
      <Navigate/>
    </Command>  
    
    <Command Name="rewindPlayer">
      <Example>Rewind content</Example>
      <ListenFor>rewind the [video] [music]</ListenFor>
      <Feedback>Rewinding</Feedback>
      <Navigate/>
    </Command> 
    
    <Command Name="increaseVolume">
      <Example>Increase volume</Example>
      <ListenFor>increase [the] volume</ListenFor>
      <ListenFor>turn up [the] volume</ListenFor>
      <Feedback>Increasing volume</Feedback>
      <Navigate/>
    </Command>
    
    <Command Name="reduceVolume">
      <Example>Reduce volume</Example>
      <ListenFor>reduce [the] volume</ListenFor>
      <ListenFor>turn down [the] volume</ListenFor>
      <Feedback>Reducing volume</Feedback>
      <Navigate/>
    </Command>   
    
    <Command Name="makeFullScreen">
      <Example>make full screen</Example>
      <ListenFor>make full screen</ListenFor>
      <Feedback>Making full screen</Feedback>
      <Navigate/>
    </Command>
    
    <Command Name="exitFullScreen">
      <Example>exit full screen</Example>
      <ListenFor>exit full screen</ListenFor>
      <ListenFor>minimize full screen</ListenFor>
      <Feedback>Exiting full screen</Feedback>
      <Navigate/>
    </Command>

    <Command Name="stopPlayer">
      <Example>stop content</Example>
      <ListenFor>stop the [video] [music]</ListenFor>
      <Feedback>Stopping</Feedback>
      <Navigate/>
    </Command>   
    
    <PhraseList Label="video" />
    
    <PhraseList Label="music" />
    
   
  </CommandSet>


</VoiceCommands>

